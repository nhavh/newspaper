@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    const string title = "Thẩm mỹ viện Hoàng Anh | Thẩm mỹ viện uy tín hàng đầu tại HCM";
    const string description = "Thẩm mỹ viện Hoàng Anh với bề dày kinh nghiệm hơn 15 năm trong lĩnh vực thẩm mỹ, tự hào là địa chỉ uy tín, chất lượng được đông đảo khách hàng tin tưởng và đánh giá cao.";
    const string keywords = "Thẩm mỹ viện Hoàng Anh | Thẩm mỹ viện uy tín hàng đầu tại HCM";
    var permalink = TMV.Utilities.Globals.FrontEndUrl;
    const string type = "website";
    string thumbnail = TMV.Utilities.Globals.FrontEndUrl + "/Content/images/thammyvienhoanganh-19-01-2016.jpg";
    var cndUrl = TMV.Utilities.Globals.FrontEndUrl;
}
@section META_INC {
    @Html.Raw(TMV.Utilities.HtmlHelper.MetaTag(title, description, keywords, permalink, type, thumbnail))
}

@section H1_INC {
    <h1><a href="/">Thẩm mỹ viện Hoàng Anh</a></h1>
}

@section LW_INC {
    @Html.Action("BoxCategory", "Common")
    @Html.Action("BoxLinkWebsite", "Common")
}

@Html.Action("BoxSlider")

<section class="wrapper">
    <div class="container">
        <article>
            @Html.Action("BoxSponsor")
        </article>
        <aside>
            @Html.Action("BoxGoogleSearch", "Common")
            @Html.Action("BoxArticleView", "Common")
            @Html.Action("BoxFanpage", "Common")
        </aside>
    </div>
</section>
<section class="wrapper content-2">
    <div class="container">
        @Html.Action("BoxVideo")
        @Html.Action("BoxPicture")
    </div>
</section>
<section class="wrapper content-3">
    <div class="container">
        @Html.Action("BoxCustomer")
    </div>
</section>
<section class="wrapper content-4">
    <div class="container">
        @Html.Action("BoxQuestionAnswer", "Common")
        @Html.Action("BoxCustomerReview", "Common")
    </div>
</section>
